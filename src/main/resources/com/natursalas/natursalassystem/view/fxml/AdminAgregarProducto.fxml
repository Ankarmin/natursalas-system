<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.*?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity"
           prefHeight="378.0" prefWidth="391.0" xmlns="http://javafx.com/javafx/23.0.1"
           fx:controller="com.natursalas.natursalassystem.controller.AdminAgregarProductoController">
    <children>
        <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="white-bg" stylesheets="@../css/SedeDesign.css">
            <children>
                <Label layoutX="24.0" layoutY="24.0" styleClass="text" stylesheets="@../css/SedeDesign.css"
                       text="Agregar producto">
                    <font>
                        <Font size="24.0"/>
                    </font>
                </Label>
                <Label layoutX="44.0" layoutY="92.0" text="Nombre">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="44.0" layoutY="134.0" text="Código">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="46.0" layoutY="182.0" text="Categoría">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label layoutX="46.0" layoutY="226.0" text="Precio">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Button fx:id="agregarProducto_bttnEditar" layoutX="84.0" layoutY="296.0" mnemonicParsing="false"
                        onAction="#agregarProducto" styleClass="button-general" stylesheets="@../css/SedeDesign.css"
