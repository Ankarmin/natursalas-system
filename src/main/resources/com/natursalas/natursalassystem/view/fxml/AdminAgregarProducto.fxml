                <TextField fx:id="agregarProducto_textFieldPrecio" layoutX="142.0" layoutY="226.0" prefHeight="26.0"
                           prefWidth="194.0" styleClass="text-field-1" stylesheets="@../css/SedeDesign.css"/>
                <ComboBox fx:id="agregarProducto_comboBoxCategoria" layoutX="142.0" layoutY="182.0" prefHeight="26.0"
                          prefWidth="194.0" styleClass="text-field-1" stylesheets="@../css/SedeDesign.css"/>
            </children>
        </AnchorPane>
    </children>
